{
  "build": {
    "commands": [
      "composer install --no-dev --optimize-autoloader --no-scripts",
      "php artisan key:generate --force",
      "php artisan config:cache",
      "php artisan route:cache",
      "php artisan view:cache"
    ]
  },
  "start": "php artisan serve --host=0.0.0.0 --port=$PORT"
}